{
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build" }
  ],
  "routes": [
    {
      "src": "/(.*\\.(js|css|svg|png|jpg|jpeg|gif|ico|json|txt|woff2?))",
      "headers": { "Cache-Control": "public, max-age=31536000, immutable" }
    },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
